            android:layout_height="80dp"
            android:layout_alignParentEnd="true"
            android:background="@drawable/bg_transparent"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:weightSum="1"
            android:visibility="gone">

            <TextView
